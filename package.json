{
  "name": "@fjedi/redis-client",
  "version": "1.167.1",
  "description": "Wrapper for connecting to redis server",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "private": false,
  "scripts": {
    "test": "env-cmd -f .env --silent jest",
    "test:watch": "env-cmd -f .env jest --watchAll",
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/redis-client.git"
  },
  "keywords": [
    "fjedi"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/redis-client/issues"
  },
  "homepage": "https://github.com/fjedi/redis-client#readme",
  "dependencies": {
    "@types/redis": "2.8.32",
    "bluebird": "3.7.2",
    "redis": "3.1.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.36",
    "@types/expect": "24.3.0",
    "@types/jest": "27.4.1",
    "@types/node": "14.18.12",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "dotenv": "10.0.0",
    "env-cmd": "10.1.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "prettier": "2.6.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3"
  }
}
